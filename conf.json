{
  "applications": {
    "app": {
      "executable": "out",
      "type": "external",
      "environment": {
        "SQLITE_FILE": "db/sqlite.db"
      },
      "processes": {
        "spare": 1,
        "max": 2,
        "idle_timeout": 10
      }
    }
  },
  "listeners": {
    "*:8080": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "match": {
        "uri": [
          "/api/docs/*",
          "/*.js",
          "/*.css"
        ]
      },
      "action": {
        "share": "static$uri"
      }
    },
    {
      "action": {
        "pass": "applications/app"
      }
    }
  ]
}